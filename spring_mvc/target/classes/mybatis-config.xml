<?xml version="1.0" encoding="UTF-8"?>

<!--  이 파일의 역할은 데이터 저장빈 클래스의 객체 별칭이름을 관리한다. -->
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
  <typeAliases>
  
         <typeAlias type="com.naver.vo.MemberVO" alias="mem" />
         <!-- MemberVO빈클래스의 객체 별칭명 mem을 지정 -->
         
         <typeAlias type="com.naver.vo.BoardVO" alias="board" />
         <typeAlias alias="guest" type="com.naver.vo.GuestVO" /> 	
         <typeAlias type="com.naver.vo.ReplyVO" alias="reply" /> 	
         <typeAlias type="com.naver.vo.MessageVO" alias="message" /> 	
         <typeAlias type="com.naver.vo.UeserVO" alias="user" /> 
         <typeAlias type="com.naver.vo.GongjiVO" alias="gongji" /> 	
         
  </typeAliases>
</configuration>